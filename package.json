{
  "name": "yckuo",
  "description": "Personal website, portfolio, playground and corner on the internet of YuCheng Kuo",
  "authors": "YuCheng Kuo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@7.12.2",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:content": "contentlayer dev",
    "dev:svelte": "vite dev",
    "build": "run-s build:*",
    "build:content": "contentlayer build --clearCache",
    "build:svelte": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint . --fix",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yuchengkuo/yckuo"
  },
  "dependencies": {
    "@markdoc/markdoc": "^0.1.13",
    "@motionone/dom": "^10.14.2",
    "@splitbee/web": "^0.3.0",
    "class-transformer": "^0.5.1",
    "cloudinary": "^1.32.0",
    "cloudinary-build-url": "^0.2.4",
    "contentlayer": "0.2.7",
    "date-fns": "^2.29.3",
    "lodash.throttle": "^4.1.1",
    "motion": "^10.14.2",
    "nprogress": "^0.2.0",
    "snd-lib": "^1.1.1",
    "sveltejs-markdoc": "^0.1.1"
  },
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@sveltejs/adapter-auto": "^1.0.0-next.85",
    "@sveltejs/kit": "^1.0.0-next.528",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^17.0.45",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "js-yaml": "^4.1.0",
    "node-html-parser": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "reading-time": "^1.5.0",
    "string-replace-async": "^3.0.2",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-confetti": "^1.2.0",
    "svelte-inview": "^3.0.1",
    "svelte-preprocess": "^4.10.7",
    "svelte-seo": "^1.4.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.1",
    "vite-plugin-windicss": "^1.8.8",
    "windicss": "^3.5.6"
  }
}
