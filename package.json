{
  "name": "yckuo",
  "description": "Personal website, portfolio, playground and corner on the internet of YuCheng Kuo",
  "authors": "YuCheng Kuo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@7.8.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:content": "contentlayer dev",
    "dev:svelte": "vite dev",
    "build": "run-s build:*",
    "build:content": "contentlayer build --clearCache",
    "build:svelte": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint . --fix",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuchengkuo/yckuo"
  },
  "dependencies": {
    "@markdoc/markdoc": "^0.1.7",
    "@motionone/dom": "^10.14.1",
    "@splitbee/web": "^0.3.0",
    "class-transformer": "^0.5.1",
    "cloudinary": "^1.31.0",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.2.1",
    "contentlayer": "0.2.7",
    "date-fns": "^2.29.2",
    "lodash.throttle": "^4.1.1",
    "motion": "^10.14.1",
    "nprogress": "^0.2.0",
    "sharp": "^0.30.7",
    "sveltejs-markdoc": "^0.1.1"
  },
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@sveltejs/adapter-auto": "^1.0.0-next.71",
    "@sveltejs/kit": "^1.0.0-next.469",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^17.0.45",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "base64-arraybuffer": "^1.0.2",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "reading-time": "^1.5.0",
    "slugify": "^1.6.5",
    "svelte": "^3.50.0",
    "svelte-check": "^2.9.0",
    "svelte-confetti": "^1.2.0",
    "svelte-inview": "^3.0.1",
    "svelte-preprocess": "^4.10.7",
    "svelte-seo": "^1.4.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2",
    "vite": "^3.1.0",
    "vite-plugin-windicss": "^1.8.7",
    "windicss": "^3.5.6"
  }
}
